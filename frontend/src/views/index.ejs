    <script nonce="<%= nonce %>">
        // Ensure the Lucide icons render
        lucide.createIcons();


        function toggleFAQ(element) {
        const content = element.nextElementSibling;
        const svg = element.querySelector('svg');
        const paragraph = content.querySelector('p');

        // Toggle the visibility of the content
        content.classList.toggle('hidden');
        content.style.maxHeight = content.classList.contains('hidden') ? '0' : content.scrollHeight + 'px';

        // Toggle the arrow rotation
        svg.classList.toggle('rotate-180');

        // Toggle the bold text for the answer
        if (content.classList.contains('hidden')) {
            paragraph.style.fontWeight = 'normal';
        } else {
            paragraph.style.fontWeight = 'bold';
        }
    }

    function toggleTechStack(id) {
        const content = document.getElementById(id);
        const svg = content.previousElementSibling.querySelector('svg');

        // Toggle the visibility of the content
        content.classList.toggle('hidden');
        content.style.maxHeight = content.classList.contains('hidden') ? '0' : content.scrollHeight + 'px';

        // Toggle the arrow rotation
        svg.classList.toggle('rotate-180');
    }
    </script>
